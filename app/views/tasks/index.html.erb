<h1>トップページ</h1>

<%= link_to "新規作成", new_task_path, class:"new-task-link" %>

<div class="task-list">
  <% @tasks.each do |t| %>
    <div class="task-content">
      <%= link_to t.task_title, task_path(t.id), class:"task-link" %>
      <p class="task-time"><%= t.task_time %></p>
    </div>
  <% end %>
</div>